{
  "canisters": {
    "maze": {
      "main": "src/main.mo",
      "type": "motoko"
    },
    "maze_assets": {
      "dependencies": [
        "maze"
      ],
      "frontend": {
        "entrypoint": "src/maze_assets/index.js",
        "output": "canisters/maze_assets/assets"
      },
      "source": [
        "src/maze_assets/assets"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "output": "canisters/",
      "packtool": ""
    },
    "start": {
      "address": "127.0.0.1",
      "port": 8000,
      "serve_root": "canisters/maze_assets/assets"
    }
  },
  "version": 1
}
